#include<stdio.h>
void main()
{
    int i,n1,n2,n3,a[10],b[10],c[10];
    scanf("%d",&n1);
    for(i=0;i<n1;i++)
    {
        scanf("%d",&a[i]);
    }
    scanf("%d",&n2);
    for(i=0;i<n2;i++)
    {
        scanf("%d",&b[i]);
    }
    n3=n1+n2;
    for(i=0;i<n1;i++)
    {
        c[i]=a[i];
    }
    for(i=0;i<n2;i++)
    {
        c[i+n1]=b[i];
    }
    for(i=0;i<n3;i++)
    {
        printf("%d",c[i]);
    }
}
